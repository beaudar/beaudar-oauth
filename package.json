{
  "name": "beaudar-oauth",
  "version": "0.1.0",
  "description": "GitHub OAuth server for beaudar",
  "main": "dist/index.js",
  "scripts": {
    "start": "cfworker run --watch --inspect src/index.ts",
    "build": "cfworker build src/index.ts",
    "deploy": "cfworker deploy --name beaudar --route 'api.lipk.org/*' src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beaudar/beaudar-oauth"
  },
  "license": "MIT",
  "devDependencies": {
    "@cfworker/dev": "^1.6.11",
    "@types/node": "^15.6.1",
    "typescript": "^4.2.4"
  }
}
